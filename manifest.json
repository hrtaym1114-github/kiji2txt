{
  "name": "Text Extractor",
  "version": "1.0",
  "description": "Extract text from a web page.",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["/kiji2txt/content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"]
}